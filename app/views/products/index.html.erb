
    <ul class="gal">
    <% @products.each do |product| %>
        <a href="products/<%= product.id %>"><li><img src=<%= product.image %> alt=<%= product.name %> > </a>
        <h3><%= product.name %></h3>
        <p><%= product.description %></p>
        <h4>$ <%= product.price %></h4></li>
    <% end %>
    </ul>